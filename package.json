  "devDependencies": {
    "@types/express": "^4.17.17",
    "ejs": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "typeorm": "^0.3.17",
    "winston": "^3.10.0"
  }
